{
  "version": 2,
  "builds": [
    {
      "src": "/src/index.js",
      "use": "@vercel/node",
      "config": {
        "node_version": "20.x"
      }
    }
  ],
  "routes": [
    {
      "src": "/encrypted-service/api-docs",
      "dest": "/api-docs"
    },
    {
      "src": "/(.*)",
      "dest": "/src/index.js"
    }
  ]
}
